{
  "name": "ChromeSkype",
  "description": "Native looking Skype on Chrome.",
  "version": "2.3",
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "permissions": [
    "webview",
    "notifications",
    "background",
    "fileSystem",
    "browser",
    "*://api.skype.com/*",
    "*://web.skype.com/*",
    "videoCapture",
    "audioCapture",
    "desktopCapture",
    "fullscreen",
    "alarms"
  ],
  "icons": { "16": "icon-16.png", "128": "icon-128.png" }
}
